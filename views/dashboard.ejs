<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if (resumes.length > 0) { %>
      <h2>Resumes</h2>
      <ul>
        <% resumes.forEach(function(resume) { %>
          <li><%= resume.name %></li>
        <% }) %>
      </ul>
    <% } else { %>
      NO RESUMES
    <% } %>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <form action="/user/resume" method="POST" enctype="multipart/form-data">
      <div class="form-group">
        <label for="resume">Resume</label>
        <input type="file" name="resume" id="resume">
      </div>
      <button type="submit">Upload</button>
    </form>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <% if (coverLetters.length > 0) { %>
      <h2>Cover Letters</h2>
      <ul>
        <% coverLetters.forEach(function(coverLetter) { %>
          <li><%= coverLetter.name %></li>
        <% }) %>
      </ul>
    <% } else { %>
      NO COVER LETTERS
    <% } %>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <form action="/user/coverletter" method="POST" enctype="multipart/form-data">
      <div class="form-group">
        <label for="coverLetter">Cover Letter</label>
        <input type="file" name="coverLetter" id="coverLetter">
      </div>
      <button type="submit">Upload</button>
    </form>
  </div>
</div>